<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
     <title>Profile</title>
       <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
      <link rel="stylesheet" href="~/css/home.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/web_Profile_MVC.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
</head>
<body onclick="//music();">
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="homePage">
                    <img id="my-logo" src="../image/clearlove7.png">
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                        <span class="fa fa-caret-down"></span>
                    </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link " asp-area="" asp-controller="Home" asp-action="homePage">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " asp-area="" asp-controller="Home" asp-action="About">About</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " asp-area="" asp-controller="Product" asp-action="Index">Product</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " asp-area="" asp-controller="Documentation" asp-action="Index">Documentation</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " asp-area="" asp-controller="News" asp-action="Index">News</a>
                        </li>
                      <!--  <li class="nav-item">
                            <a class="nav-link " asp-area="" asp-controller="Home" asp-action="Error">Error</a>
                        </li>-->
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">

            @RenderBody()

        </main>


        <div class="scroll-pane-down-top">

            <a class="drop-down" href="javascript:void(0);" onclick="scrollClick(1000000);">

                <i class="fa fa-caret-down"></i>
            </a>
        </div>
    </div>
        <footer class="border-top footer text-muted">
            <div class="container">
    &copy; 2023 - Profile - 
    <a id="linkf1" asp-area="" asp-controller="" asp-action="About">Contact</a> -
    <a id="linkf1" asp-area="" asp-controller="Home" asp-action="">Info 👉</a> -
    <a id="linkf1" href="https://www.facebook.com/Ytttuong1">@ViewBag.infomation.fb
        <i  class="fa fa-facebook"></i>              
    </a> -
    <a id="linkf1" href="https://github.com/tuongclearlove7">@ViewBag.infomation.git
         <i class="fa fa-github"></i>
    </a> -
    <a id="linkf1" href="https://www.instagram.com/warkeryt/">@ViewBag.infomation.ins
        <i class="fa fa-instagram" ></i>
    </a> -
    <a id="linkf1" asp-area="" asp-controller="Home" asp-action=""> @ViewBag.product.address</a> -
    <a id="linkf1" asp-area="" asp-controller="Home" asp-action=""> @ViewBag.product.city</a> -
    <a id="linkf1" asp-area="" asp-controller="Home" asp-action=""> @ViewBag.product.nation</a>
        </div>
        </footer>
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/home/home.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
